{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "framework": null,
  "installCommand": "cd frontend && npm ci",
  "buildCommand": "cd frontend && GENERATE_SOURCEMAP=false npm run build",
  "outputDirectory": "frontend/build",
  "rewrites": [
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}
