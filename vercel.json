{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "framework": null,
  "installCommand": "cd frontend && YARN_PRODUCTION=false yarn install --frozen-lockfile",
  "buildCommand": "cd frontend && GENERATE_SOURCEMAP=false yarn build",
  "outputDirectory": "frontend/build",
  "rewrites": [
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}
